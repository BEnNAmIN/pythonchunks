import turtle
import math

turtle.penup()
turtle.goto(-150,-50)
turtle.pendown()
turtle.circle(50)

def double_circle():
    turtle.penup()
    turtle.forward(55)
    turtle.right(90)
    turtle.forward(50)
    turtle.left(90)
    turtle.pendown()
    turtle.circle(50)
    turtle.penup()
    turtle.forward(55)
    turtle.left(90)
    turtle.forward(50)
    turtle.right(90)
    turtle.pendown()
    turtle.circle(50)

double_circle()
double_circle()

turtle.exitonclick()